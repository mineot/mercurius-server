generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Admin {
  id        String @id @default(uuid())
  email     String @unique
  username  String @unique
  password  String
  createAt  DateTime? @default(now())
  updateAt  DateTime? @default(now())

  @@map("admins")
}
